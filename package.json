{
  "name": "nynus",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/web",
    "packages/database"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:ts": "pnpm run build:database",
    "build:watch": "pnpm run build:ts --watch --preserveWatchOutput",
    "build:database": "cd packages/database && tsc -b --preserveWatchOutput",
    "dev": "scripts\\kill-ports.bat",
    "dev:ps": "powershell -File scripts/simple-dev.ps1",
    "dev:minimal": "powershell -File scripts/dev-minimal.ps1",
    "dev:simple": "turbo run dev",
    "dev:advanced": "powershell -File scripts/dev-with-cleanup.ps1",
    "dev:verbose": "powershell -File scripts/dev-with-cleanup.ps1 -Verbose",
    "dev:skip-cleanup": "powershell -File scripts/dev-with-cleanup.ps1 -SkipCleanup",
    "kill-ports": "powershell -File scripts/kill-ports.ps1",
    "start": "powershell -File scripts/enhanced-start-dev.ps1",
    "dev:admin": "powershell -File scripts/run-as-admin.ps1",
    "prisma:studio": "powershell -File scripts/restart-prisma-studio.ps1",
    "open:prisma": "powershell -File scripts/open-prisma-studio.ps1",
    "restart:prisma": "powershell -File scripts/restart-prisma-studio.ps1",
    "restart": "powershell -File scripts/restart-app.ps1",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "docker:dev": "powershell -File scripts/docker-dev.ps1",
    "docker:prod": "powershell -File scripts/docker-prod.ps1",
    "docker:dev:start": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:stop": "docker-compose -f docker-compose.dev.yml down",
    "docker:prod:start": "docker-compose --env-file .env.docker up -d",
    "docker:prod:stop": "docker-compose down",
    "docker:build": "docker-compose build --no-cache",
    "docker:health": "make health-check"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^29.0.0",
    "@types/katex": "^0.16.7",
    "@types/node": "^20.11.30",
    "@types/papaparse": "^5.3.15",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "^10.4.18",
    "chalk": "^5.4.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.1",
    "tslib": "^2.8.1",
    "tsx": "^4.19.3",
    "turbo": "^2.5.0",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "bcryptjs": "^3.0.2",
    "exceljs": "^4.4.0",
    "katex": "^0.16.22",
    "next": "^14.1.3",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-katex": "^3.0.1"
  },
  "packageManager": "pnpm@8.15.4"
}
