{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "nest build",
    "build:ts": "tsc -b",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "dev": "concurrently \"cross-env NODE_ENV=development WEBPACK_HIDE_PROGRESS=true nest start --webpack --webpack-config webpack-hmr.config.js --watch --preserveWatchOutput\" \"prisma studio --port 8386\"",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "prisma:studio": "prisma studio --port 8386"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.13",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "^11.0.13",
    "@nestjs/platform-express": "^11.0.13",
    "@nestjs/swagger": "^11.1.1",
    "@prisma/client": "5.2.0",
    "@project/database": "workspace:*",
    "@project/dto": "workspace:*",
    "@project/entities": "workspace:*",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@types/cookie-parser": "^1.4.8",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "^1.4.7",
    "nest-winston": "1.9.4",
    "puppeteer": "^24.6.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^11.0.13",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.17.30",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "prisma": "5.2.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "webpack-merge": "^5.9.0"
  }
}